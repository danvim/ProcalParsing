/* Simulataneous Linear & Quadratic Equations */
/* _x + _y = _; _x^2 + _xy + _y^2 + _x + _y  = _ */
?->$A: ?->$B: $A/$B->$D: 
?->$A: $A/$B->$C:
?->$M: ?->$Y: ?->$X: ?->$A:
$M + ($X($D^2)) - $D$Y ->$M:
?->$B:
$B$D - $C$Y + 2$C$D$X - $A -> $Y:
?->$A: $X($C^2) + $B$C - $A -> $X:
If $M Then
	$Y + sqrt($Y^2 - 4$M$X):
	Ans/(2$M) -> $A: 
	$A display
	$C-$D$A display
	$Y/$M - $A display
Else
	$X/$Y display
IfEnd :
$C - $D*Ans
